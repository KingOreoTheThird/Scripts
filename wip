while not game:IsLoaded() do
    task.wait(1)
end



local function executeScript(url)
    while true do
        local success, err = pcall(function()
            local src = game:HttpGet(url)
            loadstring(src)()
        end)

        if success then
            break
        else
            task.wait(1)
        end
    end
end

if ArmoredRaid then
    executeScript("https://raw.githubusercontent.com/KingOreoTheThird/Scripts/refs/heads/main/Armor")
elseif AttackRaid then
    executeScript("https://raw.githubusercontent.com/KingOreoTheThird/Scripts/refs/heads/main/Attack")
elseif Missions then
    executeScript("https://raw.githubusercontent.com/KingOreoTheThird/Scripts/refs/heads/main/whatsuphomies")
else
    error("FUCK YOU GNG TURN SOMETHING ON")
end

repeat task.wait(0.1) until game:IsLoaded()

task.wait(RetryDelay)

local args = {
    [1] = "Functions",
    [2] = "Retry",
    [3] = "Add"
}

local success, result = pcall(function()
    local remotes = game:GetService("ReplicatedStorage"):WaitForChild("Assets")
        :WaitForChild("Remotes")
        :WaitForChild("GET")
    return remotes:InvokeServer(unpack(args))
end)

if success then
    print("e")
else
    print("poop")
    warn("touch")
end
